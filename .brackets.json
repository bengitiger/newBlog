{
	"sass.enabled": false,
	"path": {
			"scss/page/*.scss": {
					"sass.enabled": false,
					"sass.compiler": "ruby",
					"sass.options": {
							"sourceMap": "",
							"includePaths": [],
							"line_comments": false,
							"sourceComments": false,
							"outputDir": "../../css/",
							"outputStyle": "expended"
					},
					"brackets-minifier.on-save": false,
					"linting.collapsed": true,
					"brackets-git.gitPath": "git"
			}
	},
	"brackets-minifier.on-save-project": false,
	"brackets-minifier.js-project-minify": true,
	"brackets-minifier.css-project-minify": true,
	"brackets-indent-guides.guideColor": "#414141",
	"brackets-indent-guides.guideStyle": "solid",
	"brackets-indent-guides.hideFirst": true,
	"brackets-file-tree-exclude.excludeList": [
			"/package.json",
			"/.gitignore",
			"/.babelrc",
			"/.min.css",
			"/.min.js",
			"/run_dev",
			"/README",
			"/dist/",
			"/node_modules/"
	]
}